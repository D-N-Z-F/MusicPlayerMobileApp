<?xml version="1.0" encoding="utf-8"?>
<resources >
    <style name="tab_text" parent="Widget.Design.TabLayout">
        <item name="fontFamily">@font/bold_chakrapetch</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Cut" parent="ShapeAppearance.MaterialComponents.MediumComponent">
        <item name="cornerFamily">cut</item>
        <item name="cornerSize">12dp</item>
    </style>

    <style name="DRsp">
        <item name="android:textColor">@color/dark_red</item>
        <item name="fontFamily">@font/semibold_poppins</item>
        <item name="android:textSize">@dimen/twenty_sp</item>
        <item name="android:layout_marginTop">@dimen/twenty_dp</item>
    </style>

    <style name="Wsp">
        <item name="android:textColor">@color/white</item>
        <item name="fontFamily">@font/semibold_poppins</item>
    </style>

    <style name="Webp">
        <item name="android:textColor">@color/white</item>
        <item name="fontFamily">@font/extrabold_poppins</item>
    </style>

    <style name="Bebp">
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/extrabold_poppins</item>
    </style>

    <style name="Wmp">
        <item name="android:textColor">@color/white</item>
        <item name="fontFamily">@font/medium_poppins</item>
    </style>

    <style name="Bmp">
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/medium_poppins</item>
    </style>

    <style name="Wbcrp">
        <item name="android:textColor">@color/white</item>
        <item name="fontFamily">@font/bold_chakrapetch</item>
    </style>

    <style name="Bbcrp">
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/bold_chakrapetch</item>
    </style>

    <style name="BgModeBgCbSC" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:layout_marginBottom">@dimen/twenty_dp</item>
        <item name="boxBackgroundColor">@android:color/white</item>
        <item name="boxBackgroundMode">outline</item>
        <item name="boxStrokeColor">@android:color/transparent</item>
    </style>
</resources>